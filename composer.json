{
    "name": "jashok2/payjs-laravel",
    "description": "基于 PAYJS 的 API 开发的 Composer Laravel Package，可直接用于生产环境 https://payjs.cn",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "jashok2",
            "email": "andy@popfeng.com"
        }
    ],
    "require": {
        "illuminate/support": "~6",
        "guzzlehttp/guzzle": "~6"
    },
    "autoload": {
        "psr-4": {
            "Jashok2\\Payjs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jashok2\\Payjs\\PayjsServiceProvider"
            ],
            "aliases": {
                "Payjs": "Jashok2\\Payjs\\Facades\\Payjs"
            }
        }
    }
}
